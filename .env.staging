# Environment variables för staging-miljö
# Dessa värden används för att testa med produktionslik miljö

# App
NEXT_PUBLIC_APP_URL=https://staging.handbok.org
NEXT_PUBLIC_APP_DOMAIN=handbok.org
NEXT_PUBLIC_MARKETING_DOMAIN=staging.handbok.org

# Database (Prisma)
# Connect to Supabase via connection pooling - Staging schema
DATABASE_URL="postgresql://postgres.[project-ref]:[password]@aws-0-eu-north-1.pooler.supabase.com:6543/postgres?pgbouncer=true&schema=staging"

# Direct connection to the database. Used for migrations
DIRECT_URL="postgresql://postgres.[project-ref]:[password]@aws-0-eu-north-1.pooler.supabase.com:5432/postgres?schema=staging"

# Specificerar explicit att vi använder staging databasen
DATABASE_ENV=staging

# Miljöinställningar för Next.js
NODE_ENV=production
DEPLOYMENT_ENV=staging

# Supabase
NEXT_PUBLIC_SUPABASE_URL="https://[project-ref].supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-staging-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-staging-service-role-key"

# Auth (NextAuth - optional)
NEXTAUTH_URL=https://staging.handbok.org
NEXTAUTH_SECRET=a-random-string-for-staging-only

